{
	"name": "file-sink-remote-http",
	"version": "2.0.0",
	"description": "A file sink which accesses files via RESTful HTTP verbs",
	"main": "lib/file-sink-remote-http-browser.js",
	"browser": "lib/file-sink-remote-http-browser.js",
	"scripts": {
		"test": "node ./test/basics.mjs",
		"less-build": "npx lessc --source-map --source-map-include-source less/pages.less public/css/pages.css",
		"client-js-build": "webpack-cli --config tests.webpack.cjs; webpack-cli --config component.webpack.cjs",
		"dev-client-js-watch": "webpack-cli --watch --config tests.webpack.cjs",
		"component-js-watch": "webpack-cli --watch --config component.webpack.cjs",
		"start": "run-webhandle ./test-server.mjs",
		"dev": "parallelshell 'npm run start' 'npm run dev-client-js-watch' 'npm run component-js-watch'"
	},
	"keywords": [],
