		"file-sink": "^1.0.12",
		"file-sink-server": "^1.0.5",
		"filter-log": "^1.0.10",
		"webpack": "^5.105.2"
	},
	"files": [
		"/lib",
		"/resources",
		"initialize-webhandle-component.mjs",
		"README.md"
	],
	"dependencies": {
		"@webhandle/initialize-webhandle-component": "^1.0.2",
		"@webhandle/minimal-browser-event-emitter": "^1.0.1",
		"buffer": "^6.0.3"
